{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" }, 
    { "src": "/images/(.*)", "dest": "/api/images/$1" }, 
    { "src": "/(.*)", "dest": "/api/$1" } 
  ],
  "static": {
    "directory": "api/images", 
    "public": "/api/images" 
  }
}
